<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- ALTIMETER -->
  <model>
    <path>Aircraft/Albatros-DI/Models/Panel/instruments/alt/alt.xml</path>
    <offsets>
      <x-m>  0.429 </x-m>
      <y-m>  0.294 </y-m>
      <z-m>  0.330 </z-m>
    </offsets>
  </model>
  
  <!-- TURN -->
  <model>
    <path>Aircraft/Albatros-DI/Models/Panel/instruments/turn/turn.xml</path>
    <offsets>
      <x-m> 0.463 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.382 </z-m>
    </offsets>
  </model>
  
  <!-- FUEL SELECTOR -->
  <model>
    <path>Aircraft/Albatros-DI/Models/Panel/instruments/fuelselector/fuelselector.xml</path>
    <offsets>
      <x-m> 0.419 </x-m>
      <y-m> 0.263 </y-m>
      <z-m> 0.178 </z-m>
    </offsets>
  </model>
  
